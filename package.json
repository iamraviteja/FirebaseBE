{
  "name": "firebasetut",
  "version": "1.0.0",
  "description": "Firebase auth and hosting example",
  "main": "index.js",
  "scripts": {
    "build:client": "webpack --config webpack.client.js --mode=production",
    "dev:build-client": "webpack --config webpack.client.js --mode=development --watch",
    "dev:build-serve": "firebase serve",
    "dev": "npm-run-all --parallel dev:*",
    "deploy:firebase": "firebase deploy",
    "deploy": "npm-run-all build:client  deploy:firebase"
  },
  "keywords": [
    "firebase",
    "authorization",
    "firebase",
    "hosting"
  ],
  "author": "Ravi Teja Darapu",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "babel-loader": "^8.0.6",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "webpack": "^4.41.2"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  }
}
